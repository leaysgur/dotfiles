{
  "theme": "Maple Dark",
  "ui_font_size": 12,
  "buffer_font_features": { "calt": false },
  "show_whitespaces": "all",
  "soft_wrap": "editor_width",
  "tab_size": 2,

  "gutter": {
    "code_actions": false,
    "folds": false,
    "runnables": false,
    "breakpoints": false,
    "line_numbers": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "collaboration_panel": { "button": false },
  "notification_panel": { "button": false },
  "chat_panel": { "button": "never" },
  "git_panel": { "button": false },
  "terminal": { "button": false },
  "assistant": { "version": "2", "button": false },

  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
