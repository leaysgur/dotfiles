{
  "theme": "Aylin",
  "ui_font_size": 12,
  "buffer_font_size": 14,
  "buffer_font_features": {
    "calt": false
  },
  "show_whitespaces": "all",
  "soft_wrap": "editor_width",
  "tab_size": 2,

  "languages": {
    "Markdown-Inline": {
      "show_inline_completions": false
    },
    "Markdown": {
      "formatter": "language_server",
      "show_inline_completions": false
    }
  },

  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "collaboration_panel": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "chat_panel": {
    "button": false
  },
  "assistant": {
    "version": "2",
    "button": false
  },
  "terminal": {
    "button": false
  },

  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
